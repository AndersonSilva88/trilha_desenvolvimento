
public class Programa15 {

	public static void main(String[] args) {
		
		int num1 = 5, num2 = 9, res;
		float res2;
		
		//Soma
		res = num1 + num2;
		System.out.println("A soma de " + num1 + " + " + num2 + " é " + res);
		
		//Subtração
		res = num1 - num2;
		System.out.println("A soma de " + num1 + " - " + num2 + " é " + res);
		
		//Multiplicação
		res = num1 * num2;
		System.out.println("A soma de " + num1 + " * " + num2 + " é " + res);
		
		//Divisão inteira
		res = num1 / num2;
		System.out.println("A soma de " + num1 + " / " + num2 + " é " + res);
		
		//Divisão real
		res2 = num1 / num2;
		System.out.println("A soma de " + num1 + " / " + num2 + " é " + res2);
		
		//Modulo
		res = num1 % 2;
		System.out.println("A resto da divisão de " + num1 + " por 2 é " + res);

	}

}
